{
  "name": "smiledesignsx",
  "version": "0.1.0",
  "description": "SmileDesignsX Desktop App for Restorative Smile Simulation",
  "main": "public/electron.js",
  "author": "SmileDesignsX Team",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "three": "^0.150.1",
    "react-three-fiber": "^8.0.27",
    "zustand": "^4.3.6",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "electron": "^25.8.2",
    "electron-builder": "^24.6.0",
    "wait-on": "^7.0.1",
    "react-scripts": "5.0.1"
  },
  "build": {
    "appId": "com.smiledesignsx.desktop",
    "productName": "SmileDesignsX",
    "files": [
      "build/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}